<% if params[:role] == Settings.role.register %>
  $('#favorite-majors').html('<%= j render "registers/favorite_majors" %>');
  $('#favorite-majors').modal('show');
<% else %>
  $('#table-major').html('<%= j render @majors %>');
  $('.text-center.pag').html('<%= j paginate @majors, remote: true %>');
<% end %>
